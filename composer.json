{
  "name": "yuanzhihai/think-workflow",
  "description": "Symfony Workflow component into Thinkphp.",
  "keywords": [
    "workflow",
    "thinkphp"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "yuanzhihai",
      "email": "396751927@qq.com"
    }
  ],
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "symfony/workflow": "^6.0",
    "symfony/process": "^6.0",
    "symfony/event-dispatcher-contracts": "^3.0",
    "topthink/framework": "^6.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Ting\\Think\\Workflow\\": "src/"
    }
  },
  "extra": {
    "think": {
      "services": [
        "Ting\\Think\\Workflow\\Service"
      ],
      "config": {
        "workflow": "config/workflow.php",
        "workflow_registry": "config/workflow_registry.php"
      }
    }
  },
  "config": {
    "sort-packages": true
  }
}